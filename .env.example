# Database Configuration
DATABASE_URL="your-database-connection-string"

# Encryption Configuration
ENCRYPTION_MASTER_KEY="your-64-character-hex-encryption-key-change-this-in-production"

# Session Configuration
SESSION_SECRET="your-session-secret-change-this-in-production"
REDIS_URL="redis://localhost:6379"

# Security Configuration
CORS_ORIGIN="http://localhost:5173"
RATE_LIMIT_MAX=60

# OpenAI Configuration
OPENAI_API_KEY="your-openai-api-key"

# AWS Configuration (for Polly)
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"
AWS_REGION="us-west-2"

# Application Environment
NODE_ENV="development"

# Security Features
ENABLE_MFA=true
ENABLE_AUDIT_LOGGING=true
ENABLE_DATA_ENCRYPTION=true

# Logging Configuration
LOG_LEVEL="info"
AUDIT_LOG_FILE="./logs/security-audit.log"

# Data Privacy Configuration
DATA_RETENTION_DAYS=2555
EXPORT_URL_EXPIRY_DAYS=7
DELETION_GRACE_PERIOD_DAYS=30

# Multi-Factor Authentication
MFA_ISSUER="Inferno AI"
MFA_ALGORITHM="SHA1"
MFA_DIGITS=6
MFA_PERIOD=30

# PubMed Research Integration
PUBMED_API_KEY=""
PUBMED_EMAIL="your-email@example.com"

# Clinical Protocols
CLINICAL_DATA_RETENTION_YEARS=7
HIPAA_COMPLIANCE_MODE=true